{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "functions": {
    "main.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  },
  "regions": ["bom1", "cdg1"],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "font-src 'self' https://github.githubassets.com https://fonts.googleapis.com https://fonts.gstatic.com;"
        }
      ]
    }
  ]
}
